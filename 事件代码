国家代码在...\config\world_names.csv   Ctrl+F 搜索要找的国家
省份地区代码在...\config\province_names.csv   Ctrl+F 搜索要找的省份地区
民族代号在...\config\world_names.csv       Ctrl+F 搜索要找的民族
--------------------------------------------------------------------------------

command = { type = pop_research_mod which = capitalists value = 50 } #资本家
command = { type = pop_research_mod which = clergymen value = 25 } #牧师
command = { type = pop_research_mod which = clerks value = 25 } #职员
某职业科技贡献率%

command = { type = research_time value = -10 }       #研究需要时间-10%
command = { type = research_cost value = -10 }       #研究需要点数-10%
--------------------------------------------------------------------------------

command = { type = terminate_loans }     #所有债务一笔勾销
command = { type = work_hour_eff value = 10 }  #工作时效+10%（可增加效率，税收等）
--------------------------------------------------------------------------------

command = { type = receiveprovince which = prov }
#从某国得到某省    区地          
command = { type = receivestate which = prov when = 0 }
#从某国得到某省（全省地区）
（这里只要写省份代号就够了，国家不用写）      

command = { type = secedeprovince which = country  value = provice }
#把自己某省    区地    割让给某国
command = { type = secedestate  which = country value = prov when = 0 }
#把自己某省（全省地区）割让给某国

# 两国交战时
command = { type = control which = country value = province }
# 把某省    区地    控制权给某国
command = { type = controlstate  which = country value = prov }
# 把某省（全省地区）控制权给某国

--------------------------------------------------------------------------------
command = { type = make_satellite which = [country tag] }
#让某国成为你的卫星国
command = { type = end_satellite_mastery which = [country tag] }
#取消某国是你的卫星国地位

command = { type = make_dominion which = [country tag] }
#让某国成为你的仆从国
command = { type = end_dominion_mastery which = [country tag] }
#取消某国是你的仆从国地位

command = { type = end_satellite }        #结束你的卫星国地位
command = { type = end_dominion }      #结束你的仆从国地位

command = { type = make_colony which = [country tag] }               
#使某国沦为殖民地（作用不明？？）

--------------------------------------------------------------------------------
command = { type = minority_emigration value = X }      #少数民族移民率 
command = { type = colonial_migration value = X  }      #殖民地移民率

command = { type = social_anthropology value = yes }    
#随着时间推移，少数民族反抗意识减弱 
--------------------------------------------------------------------------------

command = { type = addcore which = provnum }        #拥有某省主权
command = { type = removecore which = provnum  }    #放弃某省主权
                #拥有西藏主权
                command = { type = addcore which = 1493 }
                command = { type = addcore which = 1494 }
                command = { type = addcore which = 1523 }
                command = { type = addcore which = 1524 }
                command = { type = addcore which = 1521 }
                command = { type = addcore which = 1522 }
                command = { type = addcore which = 1520 }
                command = { type = addcore which = 1517 }
--------------------------------------------------------------------------------

command = {type = literacy value = 0.02 }
识字率+2%

command = { type = strata_income_mod which = low value = 100 } #穷人
command = { type = strata_income_mod which = middle value = 100 } #中产阶级
command = { type = strata_income_mod which = rich value = 100 }  #富裕阶级
阶级征税率+100%

资本家红利+100%
command = { type = capitalist_bonus value = 100 }
贵族红利+100%
command = { type = aristocrat_bonus value = 100 }
(这个正确说法不叫红利，DZ大大说了是什么我忘了= =)

