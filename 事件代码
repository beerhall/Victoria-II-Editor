#政治+社会改革
command = { type = constitution which = [democracy/constitutional_monarchy/monarchy/presidential_dictatorship/proletarian_dictatorship]  }
#民主主义/君主立宪制/君主制/总统共和制/无产阶级专政     
      
command = { type = executive_designation which = [parliamentarism/hms_government/laws_by_decree]  }
#议会制/内阁制/元首制

command = { type = party_system which = [multi_party/two_party/one_party]  }
#多党制/两党制/一党制

command = { type = national_value which = [liberty/equality/order]  }
#国家价值：自由/平等/正常

command = { type = voting_rights which = [none/landed/wealth/suffrage]  } 
#选举权：无/地产/财产/普选

command = { type = public_meetings value = [yes/no]  } 
#公共集会

command = {  type = press_rights which = [state_press/censorship/free_press]  } 
#出版权：国家报道/检查制度/自由报道

command = { type = political_parties which = [none_allowed/right_to_ban/all_allowed]  } 
#组织政党：禁止/取缔/自由

command = { type = trade_unions which = [none/non_socialist/socialist/all]  } 
#工会：无/非社会主义/社会主义/自由

command = { type = minimum_wages which = [none/trinkets/low/acceptable/good]  } 
#最低工资。没有/最低限度/低/一般/高

command = { type = maximum_workhours which = [8/10/12/14/24]  } 
#24=无限制  #工作时间

command = { type = safety_regulations which = [none/trinkets/low/acceptable/good]  } 
#安全保障。没有/最低限度/低/一般/高

command = { type = health_care which = [none/trinkets/low/acceptable/good]  }  
#医疗保险。没有/最低限度/低/一般/高

command = { type = unemployment_subsidies which = [none/trinkets/low/acceptable/good]  } 
#失业补助。没有/最低限度/低/一般/高

command = { type = pension_funds which = [none/trinkets/low/acceptable/good]   } 
#养老金。没有/最低限度/低/一般/高
坏孩子（BB）-10
command = { type = badboy value = -10 }
